		"serverless-domain-manager": "^6.1.0",
		"serverless-dotenv-plugin": "^4.0.2",
		"serverless-webpack": "^5.8.0",
		"ts-loader": "^9.3.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.7.4",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0",
		"webpack-graphql-loader": "^1.0.2",
		"webpack-node-externals": "^3.0.0",
