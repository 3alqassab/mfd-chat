{
	"name": "mufeed-backend",
	"version": "0.2.0",
	"private": true,
	"scripts": {
		"start": "NODE_ENV=production nodemon src/graphql.ts",
		"dev": "NODE_ENV=development nodemon src/graphql.ts",
		"build": "webpack",
		"codegen": "graphql-codegen --config codegen.yml",
		"postinstall": "prisma generate",
		"deploy": "SLS_DEBUG=* serverless deploy",
		"domain": "SLS_DEBUG=* serverless create_domain",
		"generate:gql": "node ./scripts/generateGQLFiles.js"
	},
